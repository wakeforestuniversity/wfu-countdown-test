{
    "name": "wfu/wfu-countdown-test",
    "description": "Countdown plugin (test)",
    "require": {
        "php" : ">= 7.4",
        "wfu/wfu": "~3"
    },
    "require-dev": {
      "phpunit/phpunit": "~9",
      "phpunit/php-code-coverage":"~8"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:wakeforestuniversity/wfu-wfu.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wfu\\Wp\\Plugins\\Countdown": "src/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "master": "master",
            "dev-master" : "dev"
        }
    }
}
